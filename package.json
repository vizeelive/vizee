  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "api": "(cd api; npm start)",
    "api:ci": "(cd api; npm run ci)",
    "graphql:watch": "chokidar ./hasura/**/* -c \"npm run graphql:schema\"",
    "graphql:schema": "gq http://localhost:8080/v1/graphql -H \"X-Hasura-Admin-Secret: secret\" --introspect --format json > ./src/schema.json",
    "graphql:reset": "docker-compose down; docker volume rm vizee_db_data; ./graphql-install.sh",
    "graphql": "docker-compose up -d; (cd hasura; wait-on tcp:8080 && hasura console --endpoint http://localhost:8080 --admin-secret=secret)",
    "cypress": "cypress open --config baseUrl=http://localhost:3000",
    "cypress:fast": "cypress run --config baseUrl=http://localhost:3002,video=false",
    "cypress:test": "concurrently --success first --kill-others \"BROWSER=none PORT=3002 npm run dev\" \"wait-on http://localhost:3002 && npm run cypress:fast\"",
    "stack": "concurrently --kill-others \"npm run graphql\" \"npm run graphql:watch\" \"npm run api\" \"npm start\" \"BROWSER=none PORT=3002 npm run dev\"",
    "start": "craco start",
    "dev": "REACT_APP_MOCK=true craco start",
    "build": "craco build",
    "test": "craco test",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
